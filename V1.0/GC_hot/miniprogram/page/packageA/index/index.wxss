}

.choice-bar {
  position: absolute;
  display: flex;
  width: 100%;
  font-size: 14px;
  background-color: #fff;
  color: #495060;
}

.chioce-item {
  position: absolute;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
